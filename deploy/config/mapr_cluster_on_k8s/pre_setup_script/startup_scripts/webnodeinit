#!/bin/bash
mv /etc/systemd/system/mapr-docker.service /tmp >>/tmp/init_script.log 2>&1
cp /tmp/backup/web.service /etc/systemd/system/ >>/tmp/init_script.log 2>&1
cp /tmp/backup/web-init-script /tmp/web-init-script >>/tmp/init_script.log 2>&1
systemctl daemon reload
systemctl enable web.service
env >>/tmp/init_script.log 2>&1
exec /sbin/init
while true
do
sleep 5
done
